  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/linkify-it-tlds.git"
  },
  "keywords": [
    "linkify-it",
    "tld",
    "tlds"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^13.1.7",
    "rimraf": "^3.0.0",
    "tlds": "*",
    "typescript": "^3.7.5",
    "typescript-transform-export-interop": "^1.0.2"
  }
}
